{
  "name": "fabric-gateway",
  "version": "0.0.1",
  "description": "Node SDK client library for Hyperledger Fabric Gateway",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^12.13.0 || ^14.15.0"
  },
  "scripts": {
    "clean": "rimraf dist src/protos",
    "proto-gen": "node -e 'require(\"fs\").mkdirSync(\"src/protos\", { recursive: true })'; pbjs -t static-module $(find ../fabric-protos -name *.proto -type f) ../protos/gateway.proto -o src/protos/protos.js --keep-case",
    "proto-types": "pbts -o src/protos/protos.d.ts src/protos/protos.js",
    "build": "npm-run-all clean proto-gen proto-types compile lint copy-non-ts-source",
    "copy-non-ts-source": "node -e 'require(\"fs-extra\").copy(\"src\", \"dist\", { filter: (src, dest) => !src.endsWith(\".ts\") || src.endsWith(\".d.ts\") })'",
    "compile": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@grpc/grpc-js": "^1.2.2",
    "@grpc/proto-loader": "^0.5.5",
    "elliptic": "^6.5.3",
    "fabric-protos": "^2.2.3",
    "jsrsasign": "^10.1.4",
    "protobufjs": "^6.10.2"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/elliptic": "^6.4.12",
    "@types/jest": "^26.0.16",
    "@types/jsrsasign": "^8.0.8",
    "@types/node": "^12.19.8",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "fs-extra": "^9.0.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
